#!/bin/sh

# passing options starting with -java: to java runtime
d=''
while o=`expr "$1" : '-java:\(.*\)'` 2>/dev/null; do
    d="$d -$o"
    shift
done

x=`dirname $0`
b=`basename $0`
if j=`cygpath -m $x/$b.jar 2> /dev/null`
then
    java $d -jar $j $*
else
    java $d -jar $x/$b.jar $*
fi

