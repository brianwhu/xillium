#!/bin/sh

d=''
while expr "$1" : '-[^-].*' >/dev/null 2>&1; do
    d="$d $1"
    shift
done
if [ "$1" == "--" ]; then
    shift
fi
x=`dirname $0`
b=`basename $0`
if j=`cygpath -m $x/$b.jar 2> /dev/null`
then
    java $d -jar $j $*
else
    java $d -jar $x/$b.jar $*
fi

